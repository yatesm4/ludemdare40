            <string>/// Adjust Volume &amp; Play sound

var odds = 500;

if (irandom(odds) == odds &amp;&amp; moving)
{
    var px = obj_player.x;
    var py = obj_player.y;
    var pd = point_distance(x,y,px,py);
    var volume = 0;

    if (pd &lt; 100){
        volume = 1-((pd)/100);
    }

    var snd_index = irandom_range(1,3);
    var asset = asset_get_index("snd_voice_"+voice+"_"+string(snd_index));

    var snd = audio_play_sound(asset, 10, false);
    audio_sound_gain(snd, volume, 1000);
}
</string>
